{
  "$schema": "https://raw.githubusercontent.com/tauri-apps/tauri/dev/tooling/api/schema.json",
  "identifier": "com.example.tauriapp",
  "productName": "tauri-app",
  "version": "0.1.0",
  "build": {
    "beforeDevCommand": "pnpm run dev",
    "beforeBuildCommand": "pnpm run build",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "fullscreen": false,
        "height": 600,
        "resizable": true,
        "title": "Tauri App",
        "width": 800
      }
    ]
  }
}